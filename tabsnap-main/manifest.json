{
  "manifest_version": 3,
  "name": "TabSnap",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage" // Ensure the extension has storage permission
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "tabsnapicon16.png",
    "48": "tabsnapicon48.png",
    "128": "tabsnapicon128.png"
  }
}

